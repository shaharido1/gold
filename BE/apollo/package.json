{
  "name": "goldstar",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "clean": "RD /s './dist'",
    "start": "npm run serve",
    "build": "tsc -w",
    "build:test": "tsc -p tsconfig.apollo.test.json",
    "ts-node": "ts-node -p ./tsconfig.apollo.json ./src/server.ts",
    "build:test:watch": "npm run build:test -- -w",
    "test": "mocha -r ts-node/register -p \"./test/**/*spec.ts*\"",
    "test:watch": "npm run test -- -watch-extensions ts",
    "watch-node": "nodemon dist/server.js",
    "serve": "concurrently \"npm run build\" \"npm run watch-node\"",
    "debug": "tsc -p ./tsconfig.debug.json",
    "mongoDocker": "docker run --network host -v mongoVol:/data/db --name mongo mongo",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "lint": "tslint -c tslint.json -p tsconfig.json --fix"
  },
  "private": true,
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "dependencies": {
    "apollo-server-express": "^1.3.6",
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "class-transformer": "^0.1.9",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "graphql": "^0.13.2",
    "graphql-import": "^0.6.0",
    "graphql-redis-subscriptions": "^1.5.0",
    "graphql-tag": "^2.9.2",
    "graphql-tools": "^3.0.2",
    "graphql-yoga": "^1.14.7",
    "ioredis": "^3.2.2",
    "pub-sub": "^0.1.13",
    "reflect-metadata": "^0.1.12",
    "subscriptions-transport-ws": "^0.9.9",
    "ts-graphql-plugin": "^1.1.2",
    "type-graphql": "^0.12.1"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/chai": "^4.1.4",
    "@types/graphql": "^0.13.1",
    "@types/ioredis": "^3.2.8",
    "@types/jest": "^22.2.3",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.1.2",
    "async": "^2.6.1",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "chai": "^4.1.2",
    "concurrently": "^3.5.1",
    "eslint": "4.0.0",
    "eslint-config-airbnb-base": "11.2.0",
    "eslint-plugin-import": "2.3.0",
    "eslint-plugin-promise": "3.5.0",
    "jest": "^23.0.1",
    "mocha": "^3.4.2",
    "morgan": "^1.9.0",
    "nodemon": "^1.17.5",
    "supertest": "3.0.0",
    "ts-node": "^7.0.0",
    "tslint": "^5.10.0",
    "typescript": "^2.8.3"
  }
}
