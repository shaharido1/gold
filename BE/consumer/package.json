{
  "name": "consumer",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc -p tsconfig.consumer.json -w",
    "serve:watch": "nodemon ./dist/consumer/src/server.js goldStarDevelopment",
    "start": "concurrently \"npm run build:watch\" \"npm run serve:watch\"",
    "start:prod": "concurrently \"npm run build\" \"npm run serve\"",
    "debug": "tsc -p ./tsconfig.debug.json",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "test": "jest --coverage --verbose",
    "lint": "tslint -c tslint.json -p tsconfig.json --fix"
  },
  "private": true,
  "precommit": [
    "lint"
  ],
  "dependencies": {
    "amqplib": "^0.5.2",
    "crudutils": "^1.0.0",
    "debounce": "^1.1.0",
    "nodemon": "^1.17.5",
    "redis": "^2.8.0",
    "rsmq": "^0.8.3",
    "rsmq-promise": "^1.0.1",
    "rsmq-worker": "^0.5.2",
    "rxjs": "^6.2.0",
    "uuidv4": "^1.0.0"
  },
  "devDependencies": {
    "@types/amqplib": "^0.5.7",
    "@types/node": "^10.1.4",
    "@types/redis": "^2.8.6",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "chai": "^4.1.2",
    "concurrently": "^3.5.1",
    "eslint": "4.0.0",
    "eslint-config-airbnb-base": "11.2.0",
    "eslint-plugin-import": "2.3.0",
    "eslint-plugin-promise": "3.5.0",
    "jest": "^23.3.0",
    "mocha": "^5.2.0",
    "morgan": "^1.9.0",
    "supertest": "3.0.0",
    "ts-mocha": "^1.2.0",
    "ts-node": "^7.0.0",
    "tsc": "^1.20150623.0",
    "tslint": "^5.10.0",
    "typescript": "^2.8.3"
  }
}
