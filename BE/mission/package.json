{
  "name": "missionService",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "npm run serve:watch",
    "serve:watch": "nodemon  src/server.ts",
    "serve": "set TS_NODE_PROJECT=./tsconfig.mission.json&& ts-node ./src/server.ts",
    "build": "tsc -p tsconfig.mission.json",
    "build:watch": "npm run build -- -w",
    "test": "set TS_NODE_PROJECT=./tsconfig.mission.json&& mocha --require ts-node/register \"./src/**/*spec.ts*\"",
    "test:watch": "npm run test -- --watch --watch-extensions ts",
    "mongoDocker": "docker run --network host -v mongoVol:/data/db --name mongo mongo",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "lint": "tslint -c tslint.json -p tsconfig.json --fix"
  },
  "private": true,
  "precommit": [
    "lint",
    "test"
  ],
  "dependencies": {
    "body-parser": "^1.18.3",
    "crudutils": "^1.0.0",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "mongoose": "^5.0.17",
    "morgan": "^1.9.0",
    "ts-jest": "^22.4.6"
  },
  "devDependencies": {
    "@types/chai": "^4.1.4",
    "@types/jest": "^22.2.3",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.1.4",
    "async": "^2.6.1",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "concurrently": "^3.5.1",
    "eslint": "4.0.0",
    "eslint-config-airbnb-base": "11.2.0",
    "eslint-plugin-import": "2.3.0",
    "eslint-plugin-promise": "3.5.0",
    "jest": "^23.0.1",
    "mocha": "^3.5.3",
    "nodemon": "^1.18.3",
    "supertest": "3.0.0",
    "ts-node": "^7.0.0",
    "tslint": "^5.10.0",
    "typescript": "^2.8.3"
  }
}
